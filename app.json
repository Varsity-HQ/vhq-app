{
  "expo": {
    "name": "VarsityHQ",
    "slug": "vhq",
    "version": "1.1.6",
    "orientation": "portrait",
    "description": "A social media app for students in South African universities.",
    "userInterfaceStyle": "automatic",
    "icon": "./app/assets/icon.png",
    "backgroundColor": "#15202b",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#15202b"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location to show you relavent peers close to you when you're using the app",
        "UIBackgroundModes": ["remote-notification"]
      },
      "bundleIdentifier": "com.varsityheadquarters.varsityhq",
      "buildNumber": "1.1.6",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.varsityheadquarters.varsityhq",
      "versionCode": 9,
      "useNextNotificationsApi": true,
      "permissions": []
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends on this app."
        }
      ],
      "@react-native-firebase/app"
    ]
  }
}
